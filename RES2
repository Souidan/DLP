	<script type="text/javascript">
		<!--
         /*   <![CDATA[ */
			var COMMAND_PREFIX = 'command_';

			function submitform (actionForward)
			{
				var msgs = new Array();
				var errorMessage = validate(msgs);
				if (errorMessage.length == 0) {
					document.ProtectForm.action = actionForward;
					formSubmitter.submitWithState(document.ProtectForm);
					return true;
				} else {
					vontu.common.validation.showValidationErrors(errorMessage);
					return false;
				}

			}
					
			function validate(msgs)
			{
				var msgs = new Array();

				return msgs;

			}

			function validateNotificationAction(msgs, actionID)
			{
				var prefix = COMMAND_PREFIX + actionID + '_';
				ensureNotBlank(msgs, prefix + 'notifySubject', 'The Subject field must be filled in');
				ensureNotBlank(msgs, prefix + 'notifyBody', 'The Subject field must be filled in');
			}
					

			function setTextAreaState ( textareaElementId, booleanValue )
			{
				document.getElementById(textareaElementId).disabled = booleanValue;
			}

			
				
			

			function handleModifySubjectClicked()
			{
				var subject = document.getElementById('subject');
				if (document.forms[0].elements['value(subjectAction)'][0].checked)
				{
					subject.disabled = true;
					subject.className = 'text disabled';
				}
				else
				{
					subject.disabled = false;
					subject.className = 'text';
				}
			}

			var lastFocusedInputEl = null;
			function addSubstitutionVariable(actionID, variable, defaultFieldName)
			{
				var prefix = COMMAND_PREFIX + actionID + '_';
				if ((lastFocusedInputEl == null) || (lastFocusedInputEl.id.indexOf(prefix) == -1))
				{
					lastFocusedInputEl = document.getElementById(prefix + defaultFieldName);
				}
				lastFocusedInputEl.value = lastFocusedInputEl.value + variable;
				lastFocusedInputEl.focus();
			}

			function editPolicy(actionForward)
			{
			
				if( !this.isDirty )
				{
					showLoadingIndicator();
			        location.href = actionForward;
				}
				else
				{
					vontu.widgets.Dialog.showConfirmDialog
					({
						type: vontu.widgets.Dialog.LEAVE,
						message: 'You will lose any changes. Are you sure you would like to leave this page?',
						confirmCallback: function(){
							showLoadingIndicator();
				        	location.href = actionForward;
						}
					});
				}
			}


			function introducedIncompatibility()
			{
					return false;
			}

			function initForm()
			{
				initializeChangeDetection();
			}

			var wasOkClicked;

			function okClicked(autoAdvance, reportType)
			{
                if (!wasOkClicked)
				{
                    wasOkClicked = true;
                    var reportType = document.getElementById("reportType")
                    var isSubmitted = true;
                    if(autoAdvance && (reportType != null))
                    {
                    	isSubmitted =  submitform('UpdateExecuteResponseRule.do?value(autoAdvance)=true&value(reportType)='+reportType.value);
                    }
                    else
                    {
                    	isSubmitted = submitform('UpdateExecuteResponseRule.do');
                    }
                    if(!isSubmitted)
                    {
                    	wasOkClicked = false;
                    }
                    
				}
			}


			vontu.init.addInitMethod(initForm);

         /*   ]]>       */
